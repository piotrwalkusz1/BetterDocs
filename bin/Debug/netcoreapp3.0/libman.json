{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "diff@4.0.1",
      "destination": "wwwroot/js/diff",
      "files": [
        "dist/diff.js",
        "dist/diff.min.js"
      ]
    },
    {
      "library": "jsondiffpatch@0.3.11",
      "destination": "wwwroot/js/jsondiffpatch",
      "files": [
        "dist/jsondiffpatch.umd.js"
      ]
    },
    {
      "library": "diff-match-patch@1.0.4",
      "destination": "wwwroot/js/diff-match-patch"
    },
    {
      "library": "diff-match-patch-js-browser-and-nodejs@1.0.1",
      "destination": "wwwroot/js/diff-match-patch-js-browser-and-nodejs"
    }
  ]
}